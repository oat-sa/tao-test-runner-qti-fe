{
    "name": "@oat-sa/tao-test-runner-qti",
    "version": "3.0.0",
    "description": "TAO Test Runner QTI implementation",
    "files": [
        "dist",
        "src",
        "scss",
        "css"
    ],
    "scripts": {
        "test": "npx qunit-testrunner",
        "test:keepAlive": "npx qunit-testrunner --keepalive",
        "test:cov": "npm run build:cov && npx qunit-testrunner --cov",
        "test:dev": "NODE_ENV=development run-p test:keepAlive build:watch",
        "coverage": "nyc report",
        "coverage:html": "nyc report --reporter=lcov && open-cli coverage/lcov-report/index.html",
        "build": "rollup --config ./build/rollup.config.js",
        "build:dev": "NODE_ENV=development npm run build:watch",
        "build:cov": "rollup --config ./build/rollup.config.js --environment COVERAGE",
        "build:watch": "rollup --config ./build/rollup.config.js --watch",
        "build:scss": "node ./build/scss.js",
        "lint": "eslint src test",
        "prepare": "run-s build:scss build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oat-sa/tao-test-runner-qti-fe.git"
    },
    "keywords": [
        "tao",
        "testrunner",
        "qti",
        "fe",
        "frontend"
    ],
    "publishConfig": {
        "access": "public"
    },
    "license": "GPL-2.0",
    "bugs": {
        "url": "https://github.com/oat-sa/tao-test-runner-qti-fe/issues"
    },
    "homepage": "https://github.com/oat-sa/tao-test-runner-qti-fe#readme",
    "devDependencies": {
        "@babel/core": "^7.10.3",
        "@babel/preset-env": "^7.10.3",
        "@oat-sa/browserslist-config-tao": "^0.2.0",
        "@oat-sa/expr-eval": "1.3.0",
        "@oat-sa/rollup-plugin-wildcard-external": "^0.1.0",
        "@oat-sa/tao-core-libs": "oat-sa/tao-core-libs-fe#breaking/TDR-10/upgrade-jquery-handlebars",
        "@oat-sa/tao-core-sdk": "oat-sa/tao-core-sdk-fe#breaking/TDR-10/upgrade-jquery-handlebars",
        "@oat-sa/tao-core-shared-libs": "^1.0.0",
        "@oat-sa/tao-core-ui": "oat-sa/tao-core-ui-fe#breaking/TDR-10/upgrade-jquery-handlebars",
        "@oat-sa/tao-item-runner": "oat-sa/tao-item-runner-fe#breaking/TDR-10/upgrade-jquery-handlebars",
        "@oat-sa/tao-item-runner-qti": "oat-sa/tao-item-runner-qti-fe#breaking/TDR-10/upgrade-jquery-handlebars",
        "@oat-sa/tao-qunit-testrunner": "^1.0.3",
        "@oat-sa/tao-test-runner": "oat-sa/tao-test-runner-fe#breaking/TDR-10/upgrade-jquery-handlebars",
        "@rollup/plugin-alias": "^3.1.1",
        "@rollup/plugin-babel": "^5.0.4",
        "@rollup/plugin-node-resolve": "^8.1.0",
        "async": "0.2.10",
        "autoprefixer": "^9.6.1",
        "decimal.js": "10.1.1",
        "dompurify": "^2.0.7",
        "eslint": "^6.7.0",
        "eslint-plugin-es": "^1.4.1",
        "eslint-plugin-jsdoc": "^8.7.0",
        "gamp": "0.2.1",
        "glob": "^7.1.4",
        "glob-promise": "^3.4.0",
        "handlebars": "4.7.6",
        "interactjs": "1.3.4",
        "jquery": "2.2.4",
        "jquery-mockjax": "^2.5.1",
        "jquery-simulate": "^1.0.2",
        "lodash": "2.4.1",
        "moment": "2.11.1",
        "moment-timezone": "0.5.10",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "open-cli": "^6.0.1",
        "popper.js": "1.15.0",
        "postcss-node-sass": "^2.1.8",
        "postcss-scss": "^2.0.0",
        "qunit": "^2.10.0",
        "raphael": "2.1.4",
        "require-css": "^0.1.10",
        "requirejs-plugins": "^1.0.2",
        "rollup": "^2.18.0",
        "rollup-plugin-clear": "^2.0.7",
        "rollup-plugin-handlebars-plus": "^0.2.4",
        "rollup-plugin-istanbul": "^2.0.1",
        "select2": "3.5.1",
        "tooltip.js": "1.3.2"
    },
    "browserslist": [
        "extends @oat-sa/browserslist-config-tao"
    ]
}
